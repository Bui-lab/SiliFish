    <None Include="C:\Projects\BuiLab\SiliFish\.editorconfig" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\SiliFish\SiliFish.csproj" />
  </ItemGroup>

  <ItemGroup>
